pipeline {
    agent { label 'maven_jdk8' }
    stages {
        stage('git') {
            steps {
                git url: 'https://github.com/sujeeth36/game-of-life.git' ,
                     branch: 'master'
            }
        }
        stage('package') {
            steps {
                sh 'export PATH="/usr/lib/jvm/java-1.8.0-openjdk-amd64/bin:$PATH" && mvn package'
            }
        }
    }
}
